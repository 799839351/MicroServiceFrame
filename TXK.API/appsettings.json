{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  //服务发现相关配置
  "ServiceDiscovery": {
    //Consul服务器
    "ConsulServer": {
      "Host": "127.0.0.1",
      "Port": 8500
    },
    //健康检查地址
    "HealthCheck": {
      "Host": "127.0.0.1",
      "Port": 51645,
      "API": "/api/health",
      "Interval": 10,
      "Timeout": 10
    },
    //服务本身信息
    "ServiceInfo": {
      "ServiceName": "MyService",
      "ServiceID": "MyService_Node_ServerLocal",
      "Host": "127.0.0.1",
      "Port": 51645
    }
  }
}
